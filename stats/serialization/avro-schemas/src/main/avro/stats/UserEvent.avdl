@namespace("ru.practicum.ewm.stats.avro")
protocol UserEventProtocol {

	enum ActionTypeAvro {
		VIEW,
		REGISTER,
		LIKE
	}

	record UserActionAvro {
		int userId;
		int eventId;
		ActionTypeAvro actionType;
		timestamp_ms timestamp;
	}

	record EventSimilarityAvro {
		int eventA;
		int eventB;
		double score;
		timestamp_ms timestamp;
	}
}